class Solution {
public:
    int climbStairs(int n) {
        vector<int> memo(n+1,0);
        return helper(memo,0,n);
    }

    int helper(vector<int>& memo,int level,int n){
        if(level>n) return 0;
        if(level==n) return 1;
        if(memo[level]>0) return memo[level];
        memo[level]=helper(memo,level+1,n) +helper(memo,level+2,n);
        return memo[level];
    }
};
